{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "System",
  "id": "sensetif-app",
  "autoEnabled": true,
  "info": {
    "description": "Sensetif - IoT sensor data capture, storage, analysis and visualization.",
    "author": {
      "name": "Niclas Hedhman <niclas@hedhman.org>"
    },
    "keywords": [
      "panel",
      "template",
      "iot",
      "sensetif"
    ],
    "logos": {
      "small": "img/panels-icon.svg",
      "large": "img/panels-icon.svg"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://sensetif.com"
      },
      {
        "name": "License",
        "url": "https://github.com/BaliAutomation/sensetif-app-plugin/blob/master/LICENSE"
      }
    ],
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Projects",
      "path": "/a/sensetif-app?tab=projects",
      "role": "Viewer",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "page",
      "name": "Plans",
      "path": "/a/sensetif-app?tab=plans",
      "role": "Admin",
      "addToNav": true
    },
    {
      "type": "page",
      "name": "Notifications",
      "path": "/a/sensetif-app?tab=notifications",
      "role": "Viewer",
      "addToNav": true
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=8.0.0",
    "grafanaVersion": "8.0.0",
    "plugins": []
  }
}
